    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@twurple/api": "^7.0.6",
    "axios": "^1.6.4",
    "canvacord": "^6.0.0",
    "colorette": "^2.0.20",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "discord.js": "^14.14.1",
    "enhanced-ms": "^2.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
