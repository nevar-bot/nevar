    "jimp": "^0.22.10",
    "lodash": "^4.17.21",
    "mathjs": "^12.2.1",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "mongoose": "^8.0.3",
    "node-emoji": "^2.1.3",
    "node-schedule": "^2.1.1",
    "pug": "^3.0.2",
    "toml": "^3.0.0",
    "topgg-autoposter": "^2.0.1"
  },
  "keywords": [
    "discord",
    "discord.js",
    "discord-bot",
    "open-source",
    "bot",
    "multipurpose"
